{
  "printWidth": 120,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": [
        "**/*.md"
      ],
      "options": {
        "tabWidth": 2,
        "useTabs": false,
        "singleQuote": false
      }
    }
  ]
}